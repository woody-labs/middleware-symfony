{
    "name": "woody/middleware-symfony",
    "description": "Middleware PSR-15 compliant which dispatch request to Symfony 4.x",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastien Malot",
            "email": "sebastien@malot.fr"
        }
    ],
    "require": {
        "php": "^7.1",
        "symfony/http-kernel": "^4.0",
        "woody/http-server-middleware": "dev-master"
    },
    "require-dev": {
        "doctrine/orm": "^2.6"
    },
    "suggest": {
        "doctrine/orm": "Support Doctrine ORM"
    },
    "autoload": {
        "psr-4": {
            "Woody\\Middleware\\Symfony\\": "src"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
}
